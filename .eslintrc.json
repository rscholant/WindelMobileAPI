{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
      ],
      "plugins": [
        "prettier"
      ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error",
        "class-methods-use-this": "off",
        "no-param-reassign": "off",
        "camelcase": "off",
        "no-console": ["error", {"allow": ["warn", "error", "info"]}],
        "no-unused-vars": "off",
        "strict": "off",
        "prefer-destructuring": ["error", {
          "array": false,
          "object": true
        }, {
          "enforceForRenamedProperties": false
        }]
      }
}
